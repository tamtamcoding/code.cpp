namespace Triangle
{
  bool isTriangle(int a, int b, int c)
  {
    if(unsigned(a) + unsigned(b)<=unsigned(c) || unsigned(b)+unsigned(c)<=unsigned(a) || unsigned(c)+unsigned(a)<=unsigned(b)){
      
  
      return false;
      }
    else if(a<=0 || b<=0 ||c<=0){
      return false;
    }
   
   
    else
      return true;
  }
};
