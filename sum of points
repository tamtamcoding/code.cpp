int points(const std::array<std::string, 10>& games) {
   
  int x =0;
  int y=0;
  int score=0;
  
  for (int i=0; i<games.size(); i++)
    {
    
    x= games[i].at(0);
    y=games[i].at(2);
    
    if (x>y)
      {
      score+=3;
    }
    if(x==y)
      {
      score+=1;
    }
    
  }
  
  
  
  
  return score;
  
}
